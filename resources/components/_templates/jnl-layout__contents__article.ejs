<!-- 'data-pane-target' property would be called from 'data-pane-href' property. -->
<article data-pane-target="demo-pane-third" class="jnl-layout__contents__article jsc-jnl-layout__pane">
  <h2 class="jnl-layout__contents__article__headline jsc-layout__pane__headline">Lorem ipsum dolor sit amet</h2>

  <div class="jnl-layout__contents__article__sections">
    <%# TODO:
    <ul class="jnl-notifications">
      <li class="information">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</li>
      <li class="warning">Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.</li>
      <li class="error">Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur.</li>
    </ul>
    %>

    <section class="jnl-layout__contents__article__sections__section">
      <h3 class="jnl-layout__contents__article__sections__section__headline">Lorem ipsum dolor sit amet, consectetur adipisicing elit.</h3>

      <p>This is paragraph. Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.<br>This is breaked with br tag. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur.</p>
      <p>This is the next paragraph. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.</p>
    </section>

    <section class="jnl-layout__contents__article__sections__section">
      <h3 class="jnl-layout__contents__article__sections__section__headline">Form Fields</h3>
      <form>
        <% var formfieldStyleClasses = ['', '--s', '--l', '--xl']; %>

        <% formfieldStyleClasses.forEach(function(key) { %>
          <fieldset class="jnl-formfield">
            <legend class="jnl-formfield__label">Input<%= key %></legend>
            <input type="text" value="Text Input" class="jnl-formfield__input<%= key %>">
          </fieldset>
        <% }); %>

        <% formfieldStyleClasses.forEach(function(key) { %>
          <fieldset class="jnl-formfield">
            <legend class="jnl-formfield__label">Textarea<%= key %></legend>
            <textarea class="jnl-formfield__textarea<%= key %>">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</textarea>
          </fieldset>
        <% }); %>

        <% formfieldStyleClasses.forEach(function(key) { %>
          <fieldset class="jnl-formfield">
            <legend class="jnl-formfield__label">Select</legend>
            <span class="jnl-formfield__select<%= key %> jsc-jnl-formfield__select">
              <select>
                <option value=""></option>
                <option value="option-1st">option-1st</option>
                <option value="option-2nd">option-2nd</option>
                <option value="option-3rd">option-3rd</option>
              </select>
            </span>
          </fieldset>
        <% }); %>

        <fieldset class="jnl-formfield">
          <legend class="jnl-formfield__label">Input Checkbox</legend>

          <ul class="jnl-formfield__checkboxes">
            <% for (var i = 0; i < 3; i++) { %>
              <li>
                <label class="jnl-formfield__checkbox jsc-jnl-formfield__checkbox">
                  <input type="checkbox" value="Checkbox Input">
                  <span>Checkbox Input</span>
                </label>
              </li>
            <% } %>
          </ul>
        </fieldset>

        <fieldset class="jnl-formfield">
          <legend class="jnl-formfield__label">Input Radio</legend>
          <ul class="jnl-formfield__radios">
            <% for (var i = 0; i < 3; i++) { %>
              <li>
                <label class="jnl-formfield__radio jsc-jnl-formfield__radio">
                  <input type="radio" value="Radio Input">
                  <span>Radio Input</span>
                </label>
              </li>
            <% } %>
          </ul>
        </fieldset>

        <fieldset class="jnl-formfield">
          <legend class="jnl-formfield__label">Input File</legend>
          <span class="jnl-formfield__file__appearance">
            <input type="file" value="File Input" class="jnl-formfield__file jsc-jnl-formfield__file__input">
            <span class="jsc-jnl-formfield__file__text"><i class="jnl-icon">add</i> add file</span>
          </span>
        </fieldset>

        <ul class="jnl-buttons-list">
          <li><a class="jnl-button">Button--default</a></li>
          <li><a class="jnl-button--primary">Button--primary</a></li>
          <li><a class="jnl-button--warning">Button--warning</a></li>
          <li><a class="jnl-button--disabled" disabled>Button--disabled</a></li>
        </ul>
      </form>
    </section>

    <section class="jnl-layout__contents__article__sections__section">
      <h3 class="jnl-layout__contents__article__sections__section__headline">Table</h3>

      <table class="jnl-table">
        <thead>
          <tr>
            <th>th in thead</th>
            <td>td in thead</td>
            <td>td in thead</td>
            <td>td in thead</td>
          </tr>
        </thead>
        <tbody>
          <% for (var i = 0; i < 3; i++) { %>
            <tr>
              <th>th in tbody</th>
              <td>td in tbody</td>
              <td>td in tbody</td>
              <td>td in tbody</td>
            </tr>
          <% } %>
        </tbody>
        <tfoot>
          <tr>
            <th>th in tfoot</th>
            <td>td in tfoot</td>
            <td>td in tfoot</td>
            <td>td in tfoot</td>
          </tr>
        </tfoot>
      </table>
    </section>
  </div>
</article>
